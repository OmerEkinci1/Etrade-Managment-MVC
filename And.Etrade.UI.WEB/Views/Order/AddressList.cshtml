@model IEnumerable<And.Etrade.Core.Model.Entity.UserAddress>

@{
    ViewBag.Title = "AddressList";
}

<h2>Address List</h2>

<p>
    @Html.ActionLink("Create New Address", "CreateUserAddress")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Choose", "CreateOrder", new { id=item.ID })
        </td>
    </tr>
}

</table>
